import math

#Define a constante PI
PI= 3.1416 #4 casas decimais

#solicita o raio ao usuário
raio= float (input("Digite o raio da lata:"))

#solicita a altura ao usuário
altura= float(input("Digite a altura da lata"))


#calcula o volume
volume= PI * (raio**2) * altura

#imprime o volume
print (" o volume da lata é:", volume)

